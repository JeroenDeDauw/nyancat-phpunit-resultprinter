{
    "name": "whatthejeff/nyancat-phpunit-resultprinter",
    "description": "Nyan Cat result printer for PHPUnit",
    "keywords": ["nyan","cat","rainbow","phpunit","tests"],
    "homepage": "http://github.com/whatthejeff/nyancat-phpunit-resultprinter",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Welch",
            "email": "whatthejeff@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "whatthejeff/fab": ">=1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.8.x-dev",
        "pear-pear/pear": "1.9.4"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "NyanCat": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}